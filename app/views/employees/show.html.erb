<h1><%= @employee.to_s %></h1>
    <ul>
        <li> <%= @employee.alias%> </li>
        <li> <%= @employee.title %> </li>
        <li> <%= @employee.office%> </li>
        <br>
        <%= image_tag (@employee.img_url) %>
    </ul>
    <%= button_to "edit", edit_employee_path, method: :get %>
    <%= button_to "destroy", @employee, method: :delete%>